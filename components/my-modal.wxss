
.modal-mask{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: rgba(0,0,0,0.5);
  overflow: hidden;
  z-index: 999;
  color: #434343;
  display: flex;
  justify-content: center;
  align-items:center;
}
.modal-dialog{
  width:620rpx;
  min-height: 200rpx;
  background-color: #fff;
}